---
import "./fonts.css";
import { Head as HeadComp, type HeadProps } from "@components/head";

interface Props extends HeadProps {
  pageLang: string;
}

const { faviconPath, pageLang, pageTitle } = Astro.props;
---

<html lang={pageLang}>
  <HeadComp faviconPath={faviconPath} pageTitle={pageTitle} />
  <body>
    <slot />
  </body>
</html>
