---
export interface Props {
  seoTitle: string;
  seoDescription: string;
}

const { seoTitle, seoDescription } = Astro.props;
---

{/* Primary Meta Tags */}
<meta name="title" content={seoTitle} />
<meta name="description" content={seoDescription} />

{/* Canonical URL */}
<link rel="canonical" href={Astro.url} />
